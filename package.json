  "description": "User management system with Mongodb and Nodejs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npm run migrate:up && nodemon index.js",
    "pretest": "npm run migrate:up",
    "test": "cross-env NODE_ENV=test mocha",
