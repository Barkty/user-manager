    "express": "^4.18.3",
    "express-joi-validation": "^5.0.1",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "migrate-mongo": "^11.0.0",
    "mocha": "^10.3.0",
    "mongoose": "^8.2.0",
    "mongoose-aggregate-paginate-v2": "^1.0.7",
