{
  "include": [
    "src/**",
    "lib/**/*.js"
  ],
  "exclude": [
    "**/migrations/**",
    "src/config/**"
  ],
  "ignores": [
    "import"
  ],
  "reporter": [
    "lcov",
    "json",
    "text",
    "text-summary",
    "clover"
  ],
  "check-coverage": true,
  "branches": 60,
  "lines": 60,
  "functions": 60,
  "statements": 60
}
